# Copy each module activation xml file.  Alan ships each module disabled so you can enable the ones you actually use.
@shell \
pushd $MODULE/app/etc/modules; \
for f in *.xml; \
do \
  [ -f $PROJECT/app/etc/modules/$f ] || cp $MODULE/app/etc/modules/$f $PROJECT/app/etc/modules; \
done; \
popd